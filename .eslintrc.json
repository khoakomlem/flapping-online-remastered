{
	"env": {
		"node": true,
		"browser": true,
		"es2021": true
	},
	"extends": ["xo", "plugin:react/recommended"],
	"overrides": [
		{
			"extends": ["xo-typescript"],
			"files": ["*.ts", "*.tsx"],
			"rules": {
				"@typescript-eslint/naming-convention": "off"
			}
		}
	],
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"plugins": ["react"],
	"rules": {}
}
